                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT       0 as riga_numero, prodotti.id, prodotti.descrizione, prodotti.id_tipologia, prodotti.id_fornitoretxt, prodotti.id_fornitore, prodotti.sconto, prodotti.prezzoconfezione, prodotti.prezzounitario, prodotti.tipoconfezione, prodotti.quantita_conf, prodotti.data, 
